{
    "name": "C++ Dev with Conan & CMake",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "customizations": {
      "vscode": {
        "settings": {
          "terminal.integrated.shell.linux": "/bin/bash",
          "cmake.defaultKit": "GCC 11.5.0 x86_64-redhat-linux"
